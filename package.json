{
  "name": "blockhead",
  "version": "0.0.1",
  "scripts": {
    "dev": "npx env-cmd svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "generate": "npx npm-run-all generate:*",
    "generate:swagger-typescript-api:zapper": "swagger-typescript-api --path https://api.zapper.fi/api/json --output ./src/data/zapper/api/ --clean-output true --union-enums false --route-types true --enum-names-as-values true --extract-request-params true --extract-request-body true --modular true --single-http-client true --default-response {} --unwrap-response-data true",
    "generate:swagger-typescript-api:moralis": "swagger-typescript-api --path https://deep-index.moralis.io/api-docs/v2/swagger.json --output ./src/data/moralis/api/ --clean-output true --union-enums false --route-types true --enum-names-as-values true --extract-request-params true --extract-request-body true --single-http-client true --default-response {} --unwrap-response-data true",
    "generate:houdini": "npx houdini generate --pull-schema"
  },
  "type": "module",
  "dependencies": {
    "@3id/connect": "^0.1.6",
    "@apollo/client": "^3.2.0",
    "@ceramicnetwork/3id-did-resolver": "^1.2.7",
    "@ceramicnetwork/common": "^1.1.0",
    "@ceramicnetwork/http-client": "^1.0.7",
    "@ceramicnetwork/stream-caip10-link": "^1.0.7",
    "@ceramicnetwork/stream-tile": "^1.0.7",
    "@ceramicnetwork/streamid": "^1.0.2",
    "@ceramicstudio/idx": "^0.12.1",
    "@ceramicstudio/idx-constants": "^1.0.0",
    "@compound-finance/compound-js": "^0.2.13",
    "@connext/vector-browser-node": "^0.2.5-beta.18",
    "@connext/vector-modal": "^1.0.6-beta.6",
    "@connext/vector-sdk": "^0.0.3-beta.6",
    "@connext/vector-types": "^0.2.5-beta.18",
    "@connext/vector-utils": "^0.2.5-beta.18",
    "@ethersproject/abstract-signer": "^5.4.1",
    "@ethersproject/address": "^5.4.0",
    "@ethersproject/bignumber": "^5.4.1",
    "@ethersproject/constants": "^5.4.0",
    "@ethersproject/contracts": "^5.4.1",
    "@ethersproject/providers": "^5.4.3",
    "@ethersproject/units": "^5.4.0",
    "@ethersproject/wallet": "^5.4.0",
    "@fontsource/fira-code": "^4.5.1",
    "@fontsource/ubuntu": "^4.5.0",
    "@portis/web3": "^4.0.5",
    "@web3api/client-js": "^0.0.1-prealpha.44",
    "bip44-constants": "^65.0.0",
    "buffer": "^6.0.3",
    "dids": "^2.4.0",
    "dotenv": "^10.0.0",
    "echarts": "^5.2.1",
    "ethers": "5.4.4",
    "ethers-to-web3": "^0.1.6",
    "etherspot": "^1.12.0",
    "events": "^3.3.0",
    "graphql": "^15.5.1",
    "key-did-resolver": "^1.2.1",
    "process": "^0.11.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.2",
    "subscriptions-transport-ws": "^0.9.19",
    "svelte-apollo": "^0.4.0",
    "svelte-cryptoicon": "^0.16.1"
